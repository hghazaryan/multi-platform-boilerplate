{
    "name": "multi-platform-boilerplate",
    "version": "0.0.1",
    "description": "Multi-Platform Boilerplate",
    "author": "Hayk Ghazaryan <hayk.ghazaryan@gmail.com>",
    "main": "./src/web.js",
    "scripts": {
        "all:build": "yarn run sketch:build && yarn run server:build && yarn run web:build",
        "server:start": "node ./public/server.js",
        "server:build": "webpack --config webpack.web.config.js --env.platform=server --env.production",
        "web:build:dev": "webpack --config webpack.web.config.js --env.platform=web --env.development",
        "web:build": "webpack --config webpack.web.config.js --env.platform=web --env.production",
        "sketch:build": "skpm-build",
        "sketch:watch": "skpm-build --watch",
        "sketch:start": "skpm-build --watch --run",
        "sketch:link": "skpm-link"
    },
    "browserslist": [
        "last 2 versions",
        "ie >= 11",
        "Firefox ESR"
    ],
    "engines": {
        "node": ">=6"
    },
    "skpm": {
        "name": "Multi-Platform Library",
        "manifest": "config/sketch/manifest.json",
        "main": "Multi-Platform Library.sketchplugin"
    },
    "license": "MIT",
    "devDependencies": {
        "@skpm/builder": "^0.2.12",
        "babel-core": "^6.0.0",
        "babel-loader": "^7.1.2",
        "babel-plugin-emotion": "^8.0.12",
        "babel-plugin-styled-components": "^1.5.0",
        "babel-plugin-syntax-async-functions": "^6.13.0",
        "babel-plugin-transform-async-to-generator": "^6.24.1",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-plugin-transform-es2015-template-literals": "^6.22.0",
        "babel-plugin-transform-html-to-primitives": "^0.0.5",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-regenerator": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-airbnb": "^2.4.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-react": "^6.24.1",
        "css-loader": "^0.28.9",
        "eslint": "^4.16.0",
        "eslint-config-airbnb": "^16.1.0",
        "eslint-config-prettier": "^2.9.0",
        "eslint-plugin-import": "^2.8.0",
        "eslint-plugin-jsx-a11y": "^6.0.3",
        "eslint-plugin-no-not-accumulator-reassign": "^0.1.0",
        "eslint-plugin-prettier": "^2.6.0",
        "eslint-plugin-react": "^7.6.1",
        "extract-text-webpack-plugin": "^3.0.2",
        "lint-staged": "^6.1.0",
        "node-sass": "^4.7.2",
        "pre-commit": "^1.2.2",
        "prettier": "^1.10.2",
        "sass-loader": "^6.0.6",
        "style-loader": "^0.20.1",
        "stylelint": "^8.4.0",
        "stylelint-config-standard": "^18.0.0",
        "stylelint-config-styled-components": "^0.1.1",
        "stylelint-custom-processor-loader": "^0.4.0",
        "stylelint-processor-styled-components": "^1.2.2",
        "svg-to-jsx": "^0.0.21",
        "typescript": "^2.7.1",
        "uglifyjs-webpack-plugin": "^1.1.8",
        "webpack": "^3.10.0"
    },
    "dependencies": {
        "@fortawesome/fontawesome": "^1.1.3",
        "@fortawesome/fontawesome-free-brands": "^5.0.6",
        "@fortawesome/fontawesome-free-regular": "^5.0.6",
        "@fortawesome/fontawesome-free-solid": "^5.0.6",
        "@fortawesome/react-fontawesome": "^0.0.17",
        "babel-runtime": "^6.26.0",
        "body-parser": "^1.18.2",
        "chroma-js": "^1.3.6",
        "emotion": "^8.0.12",
        "emotion-theming": "^8.0.12",
        "express": "^4.16.2",
        "lodash": "^4.17.4",
        "prop-types": "^15.6.0",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-native": "^0.52.0",
        "react-primitives": "^0.5.0",
        "react-sketchapp": "^1.1.0",
        "react-test-renderer": "^16.2.0",
        "sprintf-js": "^1.1.1",
        "styled-components": "^3.1.4",
        "webpack-node-externals": "^1.6.0"
    },
    "peerDependencies": {
        "react": "*"
    }
}
